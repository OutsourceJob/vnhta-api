  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:watch": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:watch": "nodemon dist/main",
    "start:local:watch": "NODE_ENV=local nodemon dist/main",
    "start:staging:watch": "NODE_ENV=staging nodemon dist/main",
    "start:production:watch": "NODE_ENV=production nodemon dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "debug:watch": "nodemon --inspect -e ts,tsx --exec node -r ts-node/register ./src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "console": "node dist/console",
    "start:staging": "NODE_ENV=staging pm2 start node dist/main.js --exp-backoff-restart-delay=100",
    "start:production": "NODE_ENV=production pm2 start node dist/main.js --exp-backoff-restart-delay=100"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.4.4",
    "@nestjs/platform-ws": "^7.4.4",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.4.4",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/bluebird": "^3.5.32",
    "@types/lodash": "^4.14.159",
    "@types/ws": "^7.2.6",
    "aws-sdk": "^2.747.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "commander": "^6.1.0",
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1",
    "nestjs-console": "^3.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}